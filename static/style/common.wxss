page {
  background-color: #fff;
}

.space20 {
  height: 20rpx;
  width: 100%;
}
.space-bottom {
  width: 100%;
  height: 98rpx;
  height: calc(98rpx + constant(safe-area-inset-bottom));
  height: calc(98px + env(safe-area-inset-bottom));
}
.button-bottom-float{
    position: fixed;
    bottom: 0;
    left: 0;
    z-index: 9999;
    width: 100%;
    padding-bottom: 10px;
    padding-bottom: constant(safe-area-inset-bottom);
    padding-bottom: env(safe-area-inset-bottom);
    text-align: center;
    height: 80rpx;
    background-color: #1db991;
    color: #fff;
    font-size: 32rpx;
    line-height: 80rpx;
}
.button-to-view{
  border: 0rpx !important;
  padding: 0rpx !important;
  border-radius: 0rpx 0rpx !important;
  width: 100% !important;
}

.banner {
  height: 320rpx;
  width: 100%;
}
.banner image {
  height: 100%;
  width: 100%;
}

.swiper-tab {
  border-bottom: 1px solid #efefef;
  width: 100%;
  height: 88rpx;
  line-height: 88rpx;
  display: flex;
  flex-flow: row;
  justify-content: space-between;
  background-color: #ffffff;
  text-align: center;
}
.swiper-tab .swiper-tab-item {
  width: 100%;
  font-size: 28rpx;
  color: #282828;
  font-weight: bold;    
}
.swiper-tab .actived {
  border-bottom: 6rpx solid #03b990;
  color: #03b990;
}

.swiper_banner, .swiper_banner swiper-item, .swiper_banner swiper-item view, .swiper_banner swiper-item image{
  width: 100%!important;
  height: 100%!important;
}

.view-image-center{
  display:flex;
  justify-content: center;
}

.menu-container{
  width: 100%;
}
.menu-item {
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  height: 78rpx;
  border-bottom: 1rpx solid #edeeef;
}
/** 为了图片居中显示 **/
.menu-item .menu-image{
  display:flex;
  justify-content: center;
}
.menu-item .menu-image image{
  width: 32rpx;
  height: 32rpx;
}
.menu-item .menu-text{
  font-size: 28rpx;
}
.menu-icon{
  padding-left:30rpx;
  width: 32rpx;
}
/** border: 1rpx solid red; **/
.menu-title{
  flex: 1;
  padding-left:5rpx;
  color: #272829;
}
.menu-flag{
  text-align: right;
  padding-right: 10rpx;
  color: #656565;
}
.menu-more{
  width:32rpx;
  padding-right: 20rpx;
}

.action-edit{
  height: 52rpx;
  line-height: 52rpx;
  background-color: #03b990;
  padding: 0rpx 10rpx 0rpx 10rpx;
  min-width: 160rpx;
  text-align: center;
  color: #fff;
  font-size: 26rpx;
  border-radius: 10rpx;
}
.action-delete{
  height: 52rpx;
  line-height: 52rpx;
  background-color: #babbbc;
  padding: 0rpx 10rpx 0rpx 10rpx;
  min-width: 160rpx;
  text-align: center;
  color: #fff;
  font-size: 26rpx;
  border-radius: 10rpx;
}

.bg-header-image{
  background-image: url("data:image/png;base64,");
  background-repeat: no-repeat;
  background-position: 0 0;
  background-size: 100% 100%;
}

._item-title{
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  height: 36rpx;
}
._item-title-icon{
  height: 100%;
  width: 8rpx;
  background-color: #1db88b;
  margin-right: 10rpx;
}
._item-title-text{
  flex: 1;
  height: 100%;
  font-size: 30rpx;
  font-weight: bold;
  line-height: 36rpx;
}
._item-info{
  padding-top:30rpx;
  font-size: 28rpx; 
}
._item-info-title{
  color: #677879;
  padding-bottom: 5rpx; 
}
._item-info-content{
  color: #a1a2a3;
}

._table-item{
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  border-bottom: 1rpx solid #b3ebde;
  height: 78rpx;
}
._table-title{
  color: #1cb68f;
  background-color: #b3eade;
}
._table-flex-auto{
  flex: 1;
  text-align: center;
}
._table-flex-fixed{
  text-align: center;
}

._item-form-edit{
  display: flex;
  flex-direction: row;
  font-size: 28rpx;
  justify-content: center;
  align-items: center;
  padding-top:28rpx;
  border-bottom: 1rpx solid #dcddde;
  color: #677879;
}
._item-form-closed{
  height: 28rpx;
  width: 100%;
}
._item-form-hash-line{
  border-bottom: 1rpx solid #dcddde;
}
._item-form-none-line{
  border-bottom: 0;
}
._item-form-edit ._item-form-title{
  white-space: normal;
  text-align: left;
  padding-right: 5rpx;
}
._item-form-title-top{
  align-self: flex-start;
}
._item-form-edit ._item-form-input{
  flex: 1;
  width: 30rpx;
}
._item-form-edit ._item-form-icon{
  width: 30rpx;
  height: 30rpx;
  padding-right: 5rpx;
}
._item-form-edit ._item-form-icon image{
  width: 100%;
  height:100%;
}
._item-form-sub{
  display: flex;
  justify-content: left;
  align-items: flex-start;
}
._item-form-sub-title{
  width: 160rpx;
}
._item-form-sub-content{
  flex: 1;
}

._upload_photo{
  width: 60rpx;
  height: 60rpx;
}
._picker-container{
  border: 1rpx solid #dcddde;
  height: 50rpx;
  line-height: 50rpx;
  border-radius: 10rpx;
  display: flex;
  flex-direction: row;
  justify-content: left;
  align-items: center;
  padding-left: 24rpx; 
}
._picker-date-container{
  width: 230rpx;
}
._picker_inline{
  display: inline-block;
}
._picker-select-text{ 
  flex: 1;
  display: inline-block;
  width: 100%;
  white-space: nowrap;
  word-break: break-all;
  overflow: hidden;
}
._picker-select_icon{
  width: 28rpx;
  height: 28rpx;
  padding-left: 28rpx;
  padding-right: 20rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
._picker-select_icon image{
  width: 100%;
  height: 100%;
}

._radio{
   transform:scale(0.7);
   font-size: 36rpx;
   color: #03b990;
}
._input{
  display: inline-block;
  border: 1rpx solid #dcddde;
  border-radius: 10rpx;
}
._button-close{
  font-size: 28rpx;
  color: #fa8924;
  border: 1rpx solid #fa8924;
  border-radius: 50%;
  width: 28rpx;
  height: 28rpx;
  line-height: 28rpx;
}



.form-required{color: red}
.C656565{color: #656565}
.C999999{color: #999999}
.GB-F6F6FD{background-color: #f6f6fd}